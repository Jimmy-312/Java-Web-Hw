<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.project.javaweb.mapper.UsersMapper">
    <insert id="insert">
        INSERT INTO user (id,name) VALUES (#{id},#{name});
    </insert>

    <select id="selectById" resultType="com.project.javaweb.pojo.Users"> 
        SELECT * FROM user WHERE id = #{id}; 
    </select> 
    
    <select id="selectAllStudent" resultType="com.project.javaweb.pojo.Users"> 
        SELECT * FROM user; 
    </select>
</mapper>


